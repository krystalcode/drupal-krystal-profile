{
    "name": "krystalcode/drupal8-krystal-profile",
    "description": "The Krystal Drupal 8 install profile.",
    "type": "drupal-profile",
    "homepage": "https://github.com/krystalcode/drupal8-krystal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "krystalcode",
            "homepage": "https://github.com/krystalcode",
            "role": "Developer"
        }
    ],
    "repositories": {
        "chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    },
    "require": {
        "drupal/admin_toolbar": "^1.24",
        "drupal/advagg": "^3.5",
        "drupal/bootstrap": "^3.11",
        "drupal/chosen": "^2.5",
        "drupal/config_split": "1.x-dev",
        "drupal/email_registration": "^1.0@RC",
        "drupal/field_group": "^1.0",
        "drupal/flysystem": "^1.0",
        "drupal/fpa": "2.x-dev",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/redis": "^1.0",
        "drupal/swiftmailer": "^1.0-beta",
        "drupal/webform": "5.x-dev",
        "harvesthq/chosen": "^1.8"
    }
}
